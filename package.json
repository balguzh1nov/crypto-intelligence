{
    "name": "crypto-intelligence",
    "version": "1.0.0",
    "private": true,
    "proxy": "https://api.coingecko.com",
    "description": "Система анализа криптовалютных данных в реальном времени",
    "main": "backend/server.js",
    "scripts": {
      "start": "node backend/server.js",
      "dev:backend": "nodemon backend/server.js",
      "dev:frontend": "cd frontend && npm start",
      "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
      "install:all": "npm install && cd frontend && npm install",
      "test": "jest",
      "build": "cd frontend && npm run build"
    },
    "keywords": [
      "cryptocurrency",
      "real-time",
      "data-analysis",
      "big-data",
      "cloud-computing"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "axios": "^1.6.0",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "sqlite3": "^5.1.6",
      "ws": "^8.14.2",
      "technicalindicators": "^3.1.0",
      "node-schedule": "^2.1.1",
      "winston": "^3.11.0"
    },
    "devDependencies": {
      "concurrently": "^8.2.2",
      "jest": "^29.7.0",
      "nodemon": "^3.0.1"
    }
  }